<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.klearndemo">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name=".base.KApplication"
        >
        <!--<activity android:name=""-->
        <!--<activity android:name=""-->
        <activity
            android:name=".module.activity.RecyclerViewActivity">
        </activity>

        <activity android:name=".module.activity.CustomViewActivity">
        </activity>

        <activity android:name=".module.activity.CoordinatorLayoutActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"></action>
                <category android:name="android.intent.category.LAUNCHER"></category>
            </intent-filter>
        </activity>

    </application>


</manifest>
